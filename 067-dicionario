clientes = {
    'C1': {
        'nome': 'Pamella',
        'Sobrenome': 'Oliveira da Cruz',
    },
    'C2': {
        'nome': 'Ana Claudia',
        'Sobrenome': 'Viterale'
    }
}

for k, v in clientes.items():
    print(f'Exibindo {k}')
    for k_dados, v_dados in v.items():
        print(f'\t{k_dados} = {v_dados}')

